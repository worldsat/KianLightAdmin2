<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/del_edt_banners"
    android:layout_width="match_parent"
    android:layout_height="match_parent"

    tools:context="ir.kianlight.kianlightAdmin.Delete_ads">

    <include
        android:id="@+id/app_bar_show_ads"
        layout="@layout/show_ads_bar" />

    <TextView
        android:id="@+id/time_show"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="121dp"
        android:text="آیتم مورد نظر حذف گردید !!"
        android:textColor="@color/black"
        android:textSize="16sp" />

    <Button
        android:id="@+id/btn_home_delete_banner"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/time_show"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="47dp"
        android:background="@drawable/background_button_login_green"
        android:onClick="btn_home"
        android:text="بازگشت"
        android:textColor="@color/white" />
</RelativeLayout>
